<idea-plugin>
  <id>net.itzq.tool.plugin.id</id>
  <name>NEP Generator</name>
  <version>1.3.6</version>
  <vendor email="" url="">tzq</vendor>

  <description>
    In the IDEA Database panel, connect to the database, select the table, and right-click to automatically generate
    Mybatis Mapper code.
  <![CDATA[

    ]]>
  </description>

  <change-notes>
- 1.3.6 Supports custom templates
- 1.3.5 Supports batch generation
- 1.3.4 Update code template
- 1.3.3 Update code template
- 1.3.2 Added file export function to facilitate incremental update and packaging
- 1.3.1 Update right-click menu
- 1.3.0 Update input box layout
- 1.0.0

    <![CDATA[

    ]]>
  </change-notes>

  <idea-version since-build="173.0"/>
  <depends optional="true">com.intellij.database</depends>
  <depends>com.intellij.modules.java</depends>
  <extensions defaultExtensionNs="com.intellij">
  </extensions>
  <actions>
    <!-- Add your actions here -->
    <action id="idea-mybatis-gentool-mysql" class="net.itzq.tool.mybatisgenerator.action.MainAction"
            text="Mybatis 开发助手"
            description="mybatis generator idea plugin">
      <add-to-group group-id="DatabaseViewPopupMenu" anchor="last"/>
    </action>
    <action id="FileExportAction" class="net.itzq.tool.fileexport.FileExportAction" text="导出文件"
            description="导出文件">
      <add-to-group group-id="CutCopyPasteGroup" anchor="last"/>
    </action>
    <action id="VCSFileExportAction" class="net.itzq.tool.fileexport.FileExportAction" text="导出文件"
            description="导出文件">
      <add-to-group group-id="ChangesViewPopupMenu" anchor="last"/>
    </action>
  </actions>

</idea-plugin>
